      \        )\n\n        # Reference bits\n        self.reference_bits_path = reference_bits_path\n\
      \        self.ref_bits = self._load_reference_bits(reference_bits_path)\n  \
      \      self.offset = 0\n\n        # Packet processing\n        self.current_packet_len\
